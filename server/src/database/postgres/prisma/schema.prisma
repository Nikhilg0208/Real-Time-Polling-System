generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @unique
  name       String?
  email      String    @unique
  profileUrl String?
  gender     String?
  dob        DateTime?
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}
